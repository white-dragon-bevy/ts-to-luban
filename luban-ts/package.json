{
  "name": "@white-dragon-bevy/ts-to-luban",
  "version": "0.2.7",
  "main": "out/init.lua",
  "types": "out/index.d.ts",
  "type": "module",
  "files": [
    "out"
  ],
  "scripts": {
    "build": "rbxtsc --type game",
    "test": "npx test-cloud-testez --roots ReplicatedStorage/TS",
    "watch": "rbxtsc -w --type game",
    "ts-luban": "cd .. && .\\target\\release\\luban-gen.exe -c luban-ts/luban.config.toml",
    "ts-luban:force": "cd .. && .\\target\\release\\luban-gen.exe -c luban-ts/luban.config.toml -f",
    "ts-luban:watch": "cd .. && .\\target\\release\\luban-gen.exe -c luban-ts/luban.config.toml -w",
    "config:build": "npx luban -t all -c lua-bin -d roblox-ts-json --conf configs/luban.conf -x outputCodeDir=src/types/configs -x outputDataDir=configs/jsonConfigs -x compact=1 && npm run ts-luban:force"
  },
  "devDependencies": {
    "@rbxts/compiler-types": "3.0.0-types.0",
    "@rbxts/luban": "0.6.5",
    "@rbxts/types": "^1.0.896",
    "roblox-ts": "3.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@rbxts/compiler-types": "3.0.0-types.0"
  },
  "dependencies": {
    "@rbxts/rbx-better-signal": "^2.1.0",
    "@rbxts/rewire": "^0.3.0",
    "@rbxts/services": "^1.6.0",
    "@rbxts/test-cloud-testez": "^0.5.11",
    "@white-dragon-bevy/luban-cli": "link:D:/projects/white-dragon-bevy/luban"
  }
}
